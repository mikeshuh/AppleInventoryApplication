<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.appleinventoryapp.ChargingCableSearchController">
    <children>
        <Button fx:id="logoutButton" layoutX="25.0" layoutY="25.0" mnemonicParsing="false" onAction="#switchToLoginPage" style="-fx-background-color: clear;" text="Logout" textFill="#ff3434">
            <font>
                <Font size="27.0" />
            </font>
        </Button>
        <Button fx:id="backButton" layoutX="53.0" layoutY="607.0" mnemonicParsing="false" onAction="#switchToHomePage" style="-fx-background-radius: 15 15 15 15;" text="Back">
            <font>
                <Font size="27.0" />
            </font>
        </Button>
        <Label fx:id="titleLabel" layoutX="371.0" layoutY="57.0" text="Charging Cable Search">
            <font>
                <Font size="50.0" />
            </font>
        </Label>
        <Label fx:id="modelLabel" layoutX="391.0" layoutY="176.0" text="Model">
            <font>
                <Font size="40.0" />
            </font>
        </Label>
        <ChoiceBox fx:id="modelChoiceBox" layoutX="613.0" layoutY="176.0" prefHeight="52.0" prefWidth="236.0" style="-fx-background-radius: 15 15 15 15;" />
        <Button fx:id="searchButton" layoutX="492.0" layoutY="558.0" mnemonicParsing="false" onAction="#SearchButtonOnAction" prefHeight="58.0" prefWidth="208.0" style="-fx-background-radius: 15 15 15 15;" text="Search">
            <font>
                <Font size="30.0" />
            </font>
        </Button>
      <Label fx:id="modelLabel1" layoutX="384.0" layoutY="278.0" text="Length">
         <font>
            <Font size="40.0" />
         </font>
      </Label>
      <ChoiceBox fx:id="lengthChoiceBox" layoutX="613.0" layoutY="291.0" prefHeight="52.0" prefWidth="236.0" style="-fx-background-radius: 15 15 15 15;" />
    </children>
</AnchorPane>
